{
    "name": "banzai/banzai",
    "type": "library",
    "description": "The Banzai CMS Web Application Framework",
    "keywords": [
        "CMS",
        "Web",
        "framework",
        "banzai"
    ],
    "license": "MIT",
    "homepage": "https://www.ins.de/",
    "authors": [
        {
            "name": "INS Developers",
            "email": "info@ins.de"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-sodium": "*",
        "ext-gmp": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "flux/flux": "^6|dev-develop",
        "emscherland/phpids": "^1.3",
        "bjeavons/zxcvbn-php": "^1.3",
        "twig/twig": "^3.8",
        "psr/http-message": "^1.1",
        "psr/container": "^1.1",
        "elasticsearch/elasticsearch": "^7.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "Banzai\\": "src/Banzai"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
