{
    "name": "banzai/banzai",
    "type": "library",
    "description": "The Banzai CMS Web Application Framework",
    "keywords": [
        "CMS",
        "Web",
        "framework",
        "banzai"
    ],
    "license": "MIT",
    "homepage": "https://www.ins.de/",
    "authors": [
        {
            "name": "INS Developers",
            "email": "info@ins.de"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-sodium": "*",
        "ext-gmp": "*",
        "flux/flux": "*",
        "emscherland/phpids": "^1.2",
        "bjeavons/zxcvbn-php": "^1.3",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "autoload": {
        "psr-4": {
            "Banzai\\": "src/Banzai"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
